{
  "key": "mysql",
  "label": "MySQL",
  "description": "An open-source relational database management system (RDBMS)",
  "logo": "https://nationaldataservice.atlassian.net/wiki/download/attachments/17989668/mysql-logo.png?api=v2",
  "maintainer": "",
  "image": {
    "registry": "",
    "name": "mysql",
    "tags": [
      "5.7",
      "5.6",
      "5.5",
      "latest"
    ]
  },
  "display": "stack",
  "access": "internal",
  "config": [
    {
      "name": "MYSQL_DATABASE",
      "value": "owncloud",
      "label": "Database",
      "isPassword": false,
      "canOverride": true
    },
    {
      "name": "MYSQL_USER",
      "value": "owncloud",
      "label": "Username",
      "isPassword": false,
      "canOverride": true
    },
    {
      "name": "MYSQL_PASSWORD",
      "value": "",
      "label": "Password",
      "isPassword": true,
      "canOverride": true
    },
    {
      "name": "MYSQL_ROOT_PASSWORD",
      "value": "",
      "label": "Root Password",
      "isPassword": true,
      "canOverride": true
    }
  ],
  "ports": [
    {
      "port": 3306,
      "protocol": "http"
    }
  ],
  "repositories": [
    {
      "url": "https://github.com/mysql/mysql-server",
      "type": "git"
    }
  ],
  "readinessProbe": {
    "type": "tcp",
    "path": "",
    "port": 3306,
    "initialDelay": 30,
    "timeout": 120
  },
  "volumeMounts": [
    {
      "type": "",
      "mountPath": "/var/lib/mysql"
    }
  ],
  "resourceLimits": {
    "cpuMax": 500,
    "cpuDefault": 100,
    "memMax": 1000,
    "memDefault": 50
  },
  "developerEnvironment": "cloud9cpp",
  "tags": [
    "25"
  ],
  "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/MySQL"
}
