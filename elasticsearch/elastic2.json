{
    "key": "elasticsearch2",
    "label": "ElasticSearch",
    "image": {
		"name" : "elasticsearch",
		"tags" : ["2.2"]
	},
    "description": "A distributed search and analytics engine",
	"display": "standalone",
	"access":"internal",
	"logo" : "https://github.com/nds-org/ndslabs/raw/develop/gui/asset/png/logos/elasticsearch-logo.png",
