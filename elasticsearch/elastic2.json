    "ports": [
        { "port": 9200, "protocol": "tcp" }
    ],
    "volumeMounts":[
        { "mountPath": "/usr/share/elasticsearch/data" }
    ],
	"repositories" : [
		{
			"type" : "git",
			"url" : "https://github.com/elastic/elasticsearch"
		}
	],
