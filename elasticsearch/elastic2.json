{
    "key": "elasticsearch2",
    "label": "ElasticSearch",
    "image": {
		"name" : "elasticsearch",
		"tags" : ["2.2"]
	},
    "description": "A distributed search and analytics engine",
	"display": "standalone",
	"access":"internal",
    "ports": [
        { "port": 9200, "protocol": "tcp" }
    ],
    "volumeMounts":[
        { "mountPath": "/usr/share/elasticsearch/data" }
    ],
   "readinessProbe" : {
        "type" : "http",
        "port" : 9200,
        "path" : "/",
        "initialDelay": 30,
        "timeout" : 600
    },
    "resourceLimits": {
        "cpuMax": 200,
        "cpuDefault": 100,
        "memMax": 500,
        "memDefault": 100
    },
	"tags" : ["23"]
}
