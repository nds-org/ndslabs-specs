        "type" : "http",
        "port" : 9200,
        "path" : "/",
        "initialDelay": 30,
        "timeout" : 600
    },
    "resourceLimits": {
        "cpuMax": 200,
        "cpuDefault": 100,
        "memMax": 500,
        "memDefault": 100
    },
