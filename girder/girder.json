{
  "key": "girder",
  "label": "Girder",
  "description": "Web-based data management platform.",
  "logo": "https://nationaldataservice.atlassian.net/wiki/download/attachments/16613414/girder-logo.png",
  "image": {
    "name": "girder/girder",
    "tags": [
      "1.3.1",
      "master",
      "latest"
    ]
  },
  "display": "stack",
  "access": "external",
  "depends": [
    {
      "key": "mongo",
      "required": true
    }
  ],
  "args": [
    "-d",
    "mongodb://$(MONGO_PORT_27017_TCP_ADDR):$(MONGO_PORT_27017_TCP_PORT)/girder"
  ],
  "ports": [
    {
      "port": 8080,
      "protocol": "http"
    }
  ],
  "repositories": [
    {
      "url": "https://github.com/girder/girder",
      "type": "git"
    }
  ],
  "readinessProbe": {
    "type": "http",
    "path": "/static/img/Girder_Favicon.png",
    "port": 8080,
    "initialDelay": 5,
    "timeout": 600
  },
  "resourceLimits": {
    "cpuMax": 500,
    "cpuDefault": 100,
    "memMax": 2000,
    "memDefault": 50
  },
  "tags": [
    "20", "2", "36"
  ],
  "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Girder"
}
