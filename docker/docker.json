{
    "key": "docker",
    "label": "Docker",
    "description": "Docker command-line environment for NDS Labs",
    "logo": "https://github.com/nds-org/ndslabs/raw/develop/gui/asset/png/logos/docker-logo.png",
    "image": {
        "name": "ndslabs/dind",
        "tags": [
            "latest"
        ]
    },
    "ports": [
        {
            "port": 3000,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "display": "stack",
    "access": "external",
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 512,
        "memDefault": 50
    },
    "readinessProbe" : {
        "type" : "http",
        "path" : "/",
        "port" : 3000,
        "initialDelay": 5,
        "timeout" : 600
    },
    "tags": [
        "7", "17", "41"
    ],
    "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Docker",
	"authRequired": true,
    "privileged": true
}
