{
    "key": "videopreview",
    "label": "Video Preview Extractor",
    "description": "Clowder extractor for creating a series of preview shots from a video",
    "image": {
		"name": "ndslabs/video-preview", 
		"tags" : ["0.9.2"]
	},
	"access": "internal",
    "depends": [
        {
            "key": "rabbitmq",
            "required": true
        }
    ],
    "config":  [
        {
            "name": "RABBITMQ_EXCHANGE",
            "value": "clowder",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_VHOST",
            "value": "%2F",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_QUEUE",
            "value": "ncsa.video.preview",
            "canOverride": false
        }
    ],
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 250,
        "memDefault": 25
    }
}
