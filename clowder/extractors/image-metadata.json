{
    "key": "imagemetadata",
    "label": "Image Metadata Extractor",
    "description": "Clowder extractor for image metadata",
    "image": {
		"name": "ndslabs/image-metadatak", 
		"tags" : ["0.9.2"]
	},
    "access": "internal",
    "depends": [
        {
            "key": "rabbitmq",
            "required": true
        }
    ],
    "config":  [
        {
            "name": "RABBITMQ_EXCHANGE",
            "value": "clowder",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_VHOST",
            "value": "%2F",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_QUEUE",
            "value": "ncsa.image.metadata",
            "canOverride": false
        }
    ],
	"tags" : ["27"]
}
