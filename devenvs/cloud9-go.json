{
    "key": "cloud9go",
    "label": "Cloud9 GO",
    "image": {
		"name": "ndslabs/cloud9-go",
		"tags": ["latest"]
	}, 
    "display": "stack",
    "access": "external",
	"logo": "/asset/png/logos/golang-logo.png",
    "description": "Go development environment based on Cloud9",
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        },
        {
            "port": 8080,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace", "defaultPath":"/" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 360
    },
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
	"info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Cloud9",
	"authRequired": true,
	"tags" : ["7", "17"]
}
