{
    "key": "cloud9cpp",
    "label": "Cloud9 C/C++",
    "image": {
		"name": "ndslabs/cloud9-cpp",
		"tags": ["3.0", "latest"]
	}, 
    "display": "stack",
    "access": "external",
    "description": "Unopinionated C/C++ development environment based on Cloud9",
    "ports": [
        {
            "port": 80,
            "protocol": "http"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace", "name": "cloud9cpp" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 600
    },
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
	"tags" : ["7", "14"]
}
