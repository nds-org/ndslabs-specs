{
    "key": "cloud9rails",
    "label": "Cloud9 Rails",
    "image": {
		"name": "ndslabs/cloud9-rails",
		"tags": ["latest"]
	}, 
	"logo" : "/asset/png/logos/rails-logo.png",
    "display": "stack",
    "access": "external",
    "description": "Ruby + Rails development environment based on Cloud9",
    "ports": [
        {
            "port": 80,
            "protocol": "http"
        },
        {
            "port": 3000,
            "protocol": "http"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 600
    },
    "resourceLimits": {
        "cpuMax": 200,
        "cpuDefault": 100,
        "memMax": 200,
        "memDefault": 50
    },
	"info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Cloud9",
	"authRequired": true,
	"tags" : ["7", "19"]
}
