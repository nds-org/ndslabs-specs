{
    "key": "cloud9docker",
    "label": "Cloud9 Docker",
    "image": {
        "name": "ndslabs/cloud9-docker",
        "tags": ["latest"]
    }, 
    "display": "stack",
    "access": "external",
    "description": "Docker environment based on Cloud9",
    "logo": "/asset/png/logos/docker-logo.png",
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        },
        {
            "port": 8080,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace", "defaultPath":"/" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 360
    },
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
    "info" : "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Docker",
    "authRequired": true,
    "privileged": true,
    "tags" : ["7", "41"]
}
