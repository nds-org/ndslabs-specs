{
    "key": "cloud9docker",
    "label": "Cloud9 Docker",
    "image": {
        "name": "ndslabs/cloud9-docker",
        "tags": ["latest"]
    }, 
    "display": "stack",
    "access": "external",
    "description": "Docker environment based on Cloud9",
    "logo": "https://github.com/nds-org/ndslabs-specs/raw/develop/docker/docker-logo.png",
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 360
    },
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
    "info" : "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Docker",
    "authRequired": true,
    "privileged": true,
    "tags" : ["7", "41"]
}
