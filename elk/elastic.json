{
    "key": "elasticsearch2",
    "label": "ElasticSearch",
    "image": {
		"name" : "elasticsearch",
		"tags" : ["2.2"]
	},
    "description": "A distributed search and analytics engine",
	"display": "standalone",
	"access":"internal",
    "ports": [
        { "port": 9200, "protocol": "tcp" }
    ],
    "volumeMounts":[
        { "mountPath": "/usr/share/elasticsearch/data", "name": "elasticsearch2" }
    ],
    "resourceLimits": {
        "cpuMax": "1000",
        "cpuDefault": "100",
        "memMax": "250",
        "memDefault": "100"
    }
}
