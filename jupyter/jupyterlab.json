{
    "key": "jupyterlab",
    "label": "JupyterLab",
    "description": "Extensible computational environment for Jupyter",
    "logo": "https://github.com/nds-org/ndslabs-specs/raw/develop/jupyter/jupyter-logo.png",
    "image": {
        "name": "jupyter/minimal-notebook",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "command": [
        "jupyter", "lab"
    ],
    "args": [
        "--LabApp.token=''"
    ],
    "authRequired": true,
    "ports": [
        {
            "port": 8888,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "config": [
      {
        "name": "JUPYTER_ENABLE_LAB",
        "value": "true",
        "label": "",
        "isPassword": false,
        "canOverride": false,
        "useFrom": "",
        "setTo": ""
      }
    },
    "repositories": [
		{
			"type": "git",
			"url" : "https://github.com/jupyter/jupyterlab"
		}
	],
    "readinessProbe": {
        "type": "http",
        "path": "/static/base/images/favicon.ico",
        "port": 8888,
        "initialDelay": 5,
        "timeout": 600
    },
    "volumeMounts": [ 
		{
			"mountPath": "/home/jovyan/work"
		}
    ],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
    "info" : "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Jupyter+Notebooks",
    "tags": ["7", "21", "20", "28"]
}
