{
    "key": "jupyterlab",
    "label": "JupyterLab",
    "description": "Extensible computational environment for Jupyter",
    "logo": "https://github.com/nds-org/ndslabs/raw/develop/gui/asset/png/logos/jupyter-logo.png",
    "image": {
        "name": "ndslabs/jupyterlab",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "command": [
        "start-notebook.sh"
    ],
    "args": [
        "--NotebookApp.token=''"
    ],
    "authRequired": true,
    "ports": [
        {
            "port": 8888,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "repositories": [
		{
			"type": "git",
			"url" : "https://github.com/jupyter/jupyterlab"
		}
	],
    "readinessProbe": {
        "type": "http",
        "path": "/static/base/images/favicon.ico",
        "port": 8888,
        "initialDelay": 5,
        "timeout": 600
    },
    "volumeMounts": [ 
		{
			"mountPath": "/home/jovyan/work"
		}
    ],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
    "info" : "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Jupyter+Notebooks",
    "tags": ["7", "21", "20", "28"]
}
