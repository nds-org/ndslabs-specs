{
    "key": "toolmanager",
    "label": "Analysis Toolbox",
    "description": "Customizable server for analysis tools",
    "logo": "https://github.com/nds-org/ndslabs/raw/cddr/gui/asset/png/logos/ndslabs-badge.png",
    "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Tool+Server",
    "maintainer": "",
    "image": {
        "registry": "",
        "name": "ndslabs/toolmanager",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "ports": [
        {
            "port": 8082,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "repositories": null,
    "readinessProbe": {
        "type": "tcp",
        "path": "/tools",
        "port": 8082,
        "initialDelay": 30,
        "timeout": 120
    },
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 2000,
        "memDefault": 1000
    },
    "privileged": true
}
