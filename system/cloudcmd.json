{
    "key": "cloudcmd",
    "label": "File Manager",
    "description": "Cloud Commander file manager and editor",
    "logo": "https://nationaldataservice.atlassian.net/wiki/download/attachments/16613382/file-manager.png",
    "requiresVolume": false,
    "image": {
        "registry": "",
        "name": "coderaiser/cloudcmd",
        "tags": [
            "5.5.1-alpine",
            "5.5.1",
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "args": [ "--root", "/home/$(NAMESPACE)" ],
    "ports": [
        {
            "port": 8000,
            "protocol": "http"
        }
    ],
    "readinessProbe": {
        "type": "http",
        "path": "img/logo/cloudcmd.png",
        "port": 8000,
        "initialDelay": 5,
        "timeout": 600 
    },
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
	"info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Cloud+Commander",
    "tags": [
        "21",
        "6"
    ]
}
