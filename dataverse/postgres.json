{
    "key": "postgres",
	"access": "internal",
	"display": "standalone",
    "label": "PostgreSQL",
    "image": {
		"name": "postgres",
		"tags": ["9.3"]
	},
    "ports": [
        { "port": 5432, "protocol": "tcp" }
    ],
    "volumeMounts":[
        { "mountPath": "/var/lib/postgresql/data", "name": "postgres" }
    ],
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 250,
        "memDefault": 100
    },
	"tags" : ["25"]
}
