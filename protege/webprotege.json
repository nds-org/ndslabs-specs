{
    "key": "webprotege",
    "label": "WebProtégé",
    "description": "Ontology development environment",
    "logo": "https://nationaldataservice.atlassian.net/wiki/download/attachments/15532070/webprotege-small.gif",
    "image": {
        "name": "ndslabs/webprotege",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "depends": [
        {
            "key": "mongo",
            "required": true,
            "shareConfig": false
        }
    ],
    "ports": [
        {
            "port": 8080,
            "protocol": "http"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/protegeproject/webprotege",
            "type": "git"
        }
    ],
    "readinessProbe": {
        "type": "http",
        "path": "/images/bp_icon.png",
        "port": 8080,
        "initialDelay": 30,
        "timeout": 360
    },
    "resourceLimits": {
        "cpuMax": 200,
        "cpuDefault": 100,
        "memMax": 500,
        "memDefault": 50
    },
    "tags": [
        "16",
        "32"
    ],
    "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/WebProtege"
}
