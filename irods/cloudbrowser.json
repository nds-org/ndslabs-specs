{
    "key": "cloudbrowser",
    "label": "iRODS Cloudbrowser API",
    "image": {
		"name": "ndslabs/cloudbrowser",
		"tags": ["1.0", "latest"]
	},
    "access": "external",
    "ports": [
        {
            "port": 8009,
            "protocol": "tcp"
        }
    ],
    "config":  [
        {
            "name": "IRODS_ZONE",
            "value": "",
            "label": "iRODS Zone",
            "canOverride": true
        }
	],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 2000,
        "memDefault": 500
    }
}
