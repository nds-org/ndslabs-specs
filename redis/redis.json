{
    "key": "redis",
    "label": "Redis",
    "description": "In-memory data structure store and message broker",
    "logo": "https://github.com/nds-org/ndslabs/raw/develop/gui/asset/png/logos/redis-logo.png",
    "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Redis",
    "image": {
        "name": "redis",
        "tags": [
            "3.0",
            "latest"
        ]
    },
    "display": "stack",
    "access": "internal",
    "ports": [
        {
            "port": 6379,
            "protocol": "tcp",
            "contextPath": "/"
        }
    ],
    "readinessProbe": {
        "type": "tcp",
        "path": "",
        "port": 6379,
        "initialDelay": 10,
        "timeout": 500
    },
    "resourceLimits": {
        "cpuDefault": 100,
        "cpuMax": 200,
        "memDefault": 50,
        "memMax": 512
    },
    "tags": [
        "26"
    ],
    "volumeMounts": [
        {
            "mountPath": "/data"
        }
    ]
}
