{
    "key": "convimagemagick",
    "label": "Polyglot Converter: ImageMagick",
    "logo": "https://github.com/nds-org/ndslabs/raw/develop/gui/asset/png/logos/ndslabs-badge.png",
    "image": {
        "registry": "",
        "name": "ncsa/converters-imagemagick",
        "tags": [
            "develop"
        ]
    },
    "access": "external",
    "depends": [
        {
            "key": "rabbitmq",
            "required": true
        }
    ],
    "config": [
        {
            "name": "RABBITMQ_URI",
            "value": "amqp://guest:guest@rabbitmq:5672/%2f",
            "label": "RabbitMQ URI",
            "canOverride": true
        }
    ],
    "readinessProbe": {
        "type": "",
        "path": "",
        "port": 0,
        "initialDelay": 0,
        "timeout": 0
    },
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
    "securityContext": {},
    "authRequired": true
}
