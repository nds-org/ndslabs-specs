{
    "key": "mongo",
    "label": "MongoDB",
    "image": { 
		"name" : "mongo",
		"tags" : ["3.2.4"]
	},
    "description": "A cross-platform document-oriented NoSQL database",
	"display": "standalone",
	"access": "internal",
    "ports": [
        { "port": 27017, "protocol": "tcp" }
    ],
    "volumeMounts":[
        { "mountPath": "/data/db" }
    ],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 250,
        "memDefault": 100
    },
	"tags" : ["24"]
}
